


<model
  name="Defence Drone"
  format="MS3D"
  type="JOINT"
  path="media/Actors/artilects/"
  file="DefenceDrone.ms3d"
  scale="0.02"
  bound="2.5"
  stride="1.5"
  height="2.5"
>
  <animations>
    <animation name="stand"       start="1"  end="1"  duration="1"   />
