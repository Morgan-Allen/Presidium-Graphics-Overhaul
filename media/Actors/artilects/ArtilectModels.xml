


<model
  name="Defence Drone"
  format="MS3D"
  type="JOINT"
  path="media/Actors/artilects/"
  file="DefenceDrone.ms3d"
  scale="0.02"
  bound="2.5"
  stride="1.5"
  height="2.5"
>
  <animations rotate="90">
    <animation name="stand"       start="1"  end="1"  duration="1"   />
    <animation name="move"        start="1"  end="3"  duration="1"   />
    <animation name="move_fast"   start="1"  end="3"  duration="1"   />
    <animation name="fall"        start="3"  end="5"  duration="3"   />
    <animation name="strike"      start="1"  end="3"  duration="1"   />
    <animation name="fire"        start="1"  end="3"  duration="1"   />
  </animations>
  
  <attachPoints>
    <attachPoint function="fire" joint="core"/>
    <attachPoint function="target" joint="core"/>
  </attachPoints>
</model>


